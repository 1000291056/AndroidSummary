<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelfProgress">
        <attr name="SelfProgress_textsize" format="dimension|integer"/>/*进度字体大小*/
        <attr name="SelfProgress_type">/*进度条类型*/
            <enum name="circle" value="1"/>
            <enum name="line" value="0"/>
        </attr>
        <attr name="SelfProgress_base_color" format="color"/>/*进度条底色*/
        <attr name="SelfProgress_front_color" format="color"/>/*进度条前色*/
    </declare-styleable>
</resources>